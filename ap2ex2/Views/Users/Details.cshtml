@model User

@{
    ViewData["Title"] = "Details";
}
<div class="formBox">
<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>NickName</th>
        <th>Password</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.Name</td>
        <td>@Model.Password</td>
    </tr>
    </tbody>
</table>
</div>